        h1 {
            font-weight: bold;
        }

        .w3-bar, h1, button {
            font-family: "Montserrat", sans-serif
        }

        .fa-anchor, .fa-coffee {
            font-size: 200px
        }

        * {
            box-sizing: border-box;
        }

