            /*background-color: #224E8A;*/
            padding: 30px;
            text-align: center;
            /*font-size: 35px;
              color: black;*/
        }

        /* Create two columns/boxes that floats next to each other */
        .column {
            float: left;
            /*background: #99BACC;*/
            padding: 20px;
            height: 120px;
        }

        /* Style the list inside the menu */
        nav ul {
            list-style-type: none;
            padding: 0;
        }

        .left {
            width: 40%;
            /*background-color: #fffff0;*/
        }

        .center {
            /*background-color: #E4EBF0;*/
            width: 60%;
        }

        .right {
            background-color: #E4EBF0;
            width: 75%;
        }

        
        /* Clear floats after the columns */
        section:after {
            content: "";
            display: table;
            clear: both;
        }

        .icon {
            font-size: 180px;
            display: inline-block;
            text-align: center;
        }

        #download {
            padding-bottom: 15px;
            transition: height 0.5s;
            -webkit-transition: height 0.5s;
            overflow: hidden;
        }

        /*
        #download:hover {
            height: 270px;
        }
            */

        .littleFile {
            margin-right: 5px
        }

        /* Style the footer
            footer {
              background-color: #AAAFB4;
              padding: 20px;
              text-align: center;
              color: white;
            }*/

        .show_on_mobile {
            display: none;
        }

        .hide_on_mobile {
            display: block;
        }

        .btn {
            border-radius: 10px;
        }

        .tile {
            width: 100%;
            height: 50px;
            /*margin: 0.5%;*/
            transition: height 0.5s;
            -webkit-transition: height 0.5s;
            overflow: hidden;
            border-radius: 25px;
            cursor: pointer;
            font-size: small;
        }

        #tile1.expand {
            height: 220px;
            cursor: auto;
        }

        #tile2.expand {
            height: 220px;
            cursor: auto;
        }

        #tile3.expand {
            height: 220px;
            cursor: auto;
        }

        #tile4.expand {
            height: 130px;
            cursor: auto;
        }

        #tile5.expand {
            height: 190px;
            cursor: auto;
        }

        .icon2 {
            font-size: 180px;
            display: inline-block;
            text-align: center;
            width: 100%;
            height: 240px;
            padding-top: 30px;
        }

        header {
            width: 100%;
            height: 404px;
            background-image: url("/images/covid.jpg");
            background-repeat: no-repeat;
            background-size: cover;
            background-position: center;
            display: block;
            /*padding: 128px 16px;*/
        }

        .mobile_show {
            display: none;
        }

        .link:hover {
            color: grey
        }

        .file_column {
            display: flex;
        }

        

            /* Responsive layout - makes the two columns/boxes stack on top of each other instead of next to each other, on small screens */
            @@media (max-width: 930px) {

                h1 {
                    font-size: 30px;
                    font-weight: bold;
                }

                .hide_on_mobile {
                    display: none;
                }

                .show_on_mobile {
                    display: block;
                }

                .small_button {
                    font-size: 5px;
                }

                .small_mobile_text {
                    font-size: 20px;
                }

                .tile {
                    float: left;
                    width: 100%;
                    height: 50px;
                    margin: 0.5%;
                    transition: height 0.5s;
                    -webkit-transition: height 0.5s;
                    overflow: hidden;
                    border-radius: 25px;
                }

                #tile1.expand {
                    height: 450px;
                }

                #tile2.expand {
                    height: 430px;
                }

                #tile3.expand {
                    height: 450px;
                }

                #tile4.expand {
                    height: 120px;
                }

                #tile5.expand {
                    height: 220px;
                }
                    
                .mobile_show {
                    display: block;
                }

                .mobile_margin {
                    margin-left: 0px;
                }

                .center_on_mobile {
                    text-align: center;
                }

                .w3-half {
                    width: 100%
                }

                .file_column {
                    display: block;
                }
            }
